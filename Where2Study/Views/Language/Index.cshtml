@model IEnumerable<Where2Study.Models.jezik>

@{
    ViewBag.Title = "Index";
}

@{
    Layout = "";
}

<h2>Choose language</h2>
 <p>
    Select from @Model.Count() languages:
</p>
<ul>
    @foreach (var jezik in Model)
    {
        <li>@Html.ActionLink(jezik.ime_jezika, "", "home")</li>

    }
</ul>