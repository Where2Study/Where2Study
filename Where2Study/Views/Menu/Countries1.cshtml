@model IEnumerable<Where2Study.Models.drzava_tekst>



@{
    ViewBag.Title = "Countries";
}

<h3>@Resources.Resources.BrowseCountries</h3>

@if(@Model.Count()!=0){
<p>
    @Resources.Resources.SelectFromb @Model.Count() @Resources.Resources.countries:
</p>}

<ul>
    @foreach (var item in Model)
    {
        <li>@Html.ActionLink(item.naziv, "Cities", new {country=item.naziv})</li>
    }
</ul>

